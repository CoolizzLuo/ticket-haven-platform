{
  "extends": ["@coolizz/eslint-config"],
  "rules": {
    "react/jsx-boolean-value": "off",
    "react/require-default-props": "off",
    "linebreak-style": "off",
    "no-param-reassign": [
      "error",
      {
        "props": true,
        "ignorePropertyModificationsForRegex": ["^acc*"]
      }
    ]
  }
}
